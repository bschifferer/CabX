{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha ./backend/test frontend/test",
    "eslint": "node_modules/eslint/bin/eslint.js backend/src frontend/src",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha frontend/src/*.js backend/src/*.js",
    "install": "npm install --save-dev pre-commit"
  },
  "dependencies": {
    "@expo/vector-icons": "^8.0.0",
    "expo": "^30.0.1",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-elements": "^0.19.1",
    "react-navigation": "^2.18.2"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^5.8.0",
    "eslint-config-google": "^0.11.0",
    "istanbul": "^0.4.5",
    "metro-react-native-babel-preset": "^0.48.1",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "test",
    "eslint",
    "coverage"
  ]
}
